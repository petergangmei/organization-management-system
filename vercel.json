{
    "builds": [{
        "src": "core/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src":"build.sh",
        "use": "@vercel/static-build",
        "config":{"distDir":"static_build"}
    }
],
    
    "routes": [
        {
            "src":"/tatics/(.*)",
            "dest":"/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        }
    ]
}